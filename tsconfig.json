{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@assets/*": ["./src/assets/*"],
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content/*"],
      "@images/*": ["./src/assets/images/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@pages/*": ["./src/pages/*"],
      "@scripts/*": ["./src/scripts/*"],
      "@styles/*": ["./src/assets/styles/*"]
    }
  }
}
